[{
  "path": "$.items[*]",
  "command": "resolve",
  "resolveSettings": [
    {
      "resolveKeys": [{"keyPath": "@.refKey", "referenceKeyPath": "@.keys[*]"}],
      "referencesCollectionPath": "$.references[*]",
      "values": [
        {"targetPath": "@.referenceData", "value": "@"},
        {"targetPath": "@.categories", "value": "=fetch(@.category)"}
      ]
    }
  ]
}]